{
  "fallbackRetryDelay": "3s",
  "httpHeaders": [
    {
      "headers": {
        "Accept-Encoding": "zstd, br, gzip, deflate"
      },
      "urls": [
        "https://github.com/",
        "https://guides.github.com/",
        "https://help.github.com/",
        "https://docs.github.com/"
      ]
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "retryCount": 3,
  "retryOn429": true
}
