{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["group:all"],
  "enabledManagers": ["gomod"],
  "packageRules": [
    {
      "matchPackageNames": [
        "go.opentelemetry.io/collector.*",
        "github.com/open-telemetry/opentelemetry-collector-contrib.*"
      ],
      "groupName": "opentelemetry-collector"
    },
    {
      "matchPackageNames": [".*"],
      "excludePackageNames": [
        "go.opentelemetry.io/collector.*",
        "github.com/open-telemetry/opentelemetry-collector-contrib.*"
      ],
      "enabled": false
    }
  ]
}
